    "dotenv": "^16.0.1",
    "express": "^4.17.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.31.3",
    "slug": "^8.2.2",
    "swagger-autogen": "^2.23.0",
    "swagger-ui-express": "^4.6.0",
    "ts-jest": "^27.1.3",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
